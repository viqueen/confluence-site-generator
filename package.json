{
  "name": "confluence-site-generator",
  "version": "1.0.0",
  "description": "confluence site generator",
  "scripts": {
    "extract:data:watch": "nodemon --config nodemon-generator.json",
    "extract:data": "ts-node ./src/generator/extract.ts",
    "site:watch": "webpack serve",
    "site:generate": "webpack --config webpack.config.js",
    "format:check": "prettier --check .",
    "format": "prettier --check --write .",
    "check-updates": "npm-check-updates -u"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/viqueen/confluence-site-generator.git"
  },
  "keywords": [
    "confluence",
    "site",
    "generator"
  ],
  "author": "Hasnae R.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/viqueen/confluence-site-generator/issues"
  },
  "homepage": "https://github.com/viqueen/confluence-site-generator#readme",
  "devDependencies": {
    "@types/node": "^17.0.5",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "css-loader": "^5.2.7",
    "dotenv": "^12.0.3",
    "fs-directory": "^1.0.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "npm-check-updates": "^12.0.5",
    "prettier": "^2.5.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.3.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "dependencies": {
    "@atlaskit/adf-utils": "^14.4.0",
    "@atlaskit/atlassian-navigation": "^2.0.2",
    "@atlaskit/avatar": "^20.5.2",
    "@atlaskit/breadcrumbs": "^11.5.0",
    "@atlaskit/date": "^0.8.9",
    "@atlaskit/heading": "^0.1.5",
    "@atlaskit/icon-object": "^6.2.5",
    "@atlaskit/page": "^12.1.0",
    "@atlaskit/renderer": "^84.1.1",
    "@atlaskit/smart-card": "^17.0.0",
    "@atlaskit/spinner": "^15.1.4",
    "@atlaskit/theme": "^12.1.0",
    "axios": "^0.24.0",
    "json-ld-types": "^2.3.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-intl": "^2.9.0",
    "styled-components": "^3.4.10"
  }
}

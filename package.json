{
  "name": "confluence-site-generator",
  "version": "1.0.0",
  "description": "confluence site generator",
  "scripts": {
    "build": "tsc --watch",
    "ci:build": "tsc",
    "site:extract": "confsite extract public",
    "site:watch": "webpack serve",
    "site:generate": "webpack --config webpack.config.js",
    "site:firebase:serve": "firebase serve --port 9000",
    "site:firebase:preview": "firebase hosting:channel:deploy",
    "site:firebase:deploy": "firebase deploy --only hosting",
    "format:check": "prettier --check .",
    "format": "prettier --check --write .",
    "check-updates": "npm-check-updates -u"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/viqueen/confluence-site-generator.git"
  },
  "keywords": [
    "confluence",
    "site",
    "generator"
  ],
  "author": "Hasnae R.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/viqueen/confluence-site-generator/issues"
  },
  "homepage": "https://github.com/viqueen/confluence-site-generator#readme",
  "devDependencies": {
    "@types/node": "^17.0.12",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "confluence-content-extractor": "^1.1.0",
    "css-loader": "^5.2.7",
    "dotenv": "^16.0.1",
    "file-loader": "^6.2.0",
    "fs-directory": "^1.0.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^8.0.1",
    "nodemon": "^2.0.15",
    "npm-check-updates": "^15.2.0",
    "prettier": "^2.7.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.3.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "dependencies": {
    "@atlaskit/adf-utils": "^17.0.0",
    "@atlaskit/atlassian-navigation": "^2.2.3",
    "@atlaskit/avatar": "^21.0.0",
    "@atlaskit/breadcrumbs": "^11.6.2",
    "@atlaskit/date": "^0.9.2",
    "@atlaskit/heading": "^0.1.12",
    "@atlaskit/icon-object": "^6.2.6",
    "@atlaskit/media-core": "^33.0.0",
    "@atlaskit/page": "^12.1.3",
    "@atlaskit/renderer": "^96.0.1",
    "@atlaskit/smart-card": "^19.1.27",
    "@atlaskit/spinner": "^15.1.12",
    "@atlaskit/theme": "^12.1.9",
    "axios": "^0.27.2",
    "commander": "^9.3.0",
    "json-ld-types": "^2.3.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-intl": "^2.9.0",
    "styled-components": "^3.4.10"
  }
}
